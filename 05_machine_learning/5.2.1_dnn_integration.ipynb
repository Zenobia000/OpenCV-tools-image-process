    "def run_comprehensive_benchmark():\n",
    "    \"\"\"執行全面的DNN模組基準測試\"\"\"\n",
    "    \n",
    "    print(\"\\n🔄 執行DNN模組全面基準測試\")\n",
    "    print(\"=\" * 50)\n",
    "    \n",
    "    test_cases = [\n",
    "        (\"../assets/images/basic/face03.jpg\", \"單人臉\"),\n",
    "        (\"../assets/images/basic/faces01.jpg\", \"多人臉\"),\n",
    "        (\"../assets/images/basic/faces.png\", \"群體照片\")\n",
    "    ]\n",
    "    \n",
    "    total_tests = 0\n",
    "    successful_tests = 0\n",
    "    \n",
    "    for image_path, description in test_cases:\n",
    "        if not os.path.exists(image_path):\n",
    "            continue\n",
    "            \n",
    "        print(f\"\\n📷 測試: {description} ({os.path.basename(image_path)})\")\n",
    "        print(\"-\" * 30)\n",
    "        \n",
    "        image = load_image(image_path)\n",
    "        if image is None:\n",
    "            continue\n",
    "        \n",
